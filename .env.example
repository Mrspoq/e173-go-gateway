# E173 Go Gateway - Environment Configuration
# Copy this file to .env and update with your actual values

# Server Configuration
SERVER_PORT=8080
GIN_MODE=debug

# Database Configuration
DATABASE_URL=postgres://e173_user:e173_pass@localhost:5432/e173_gateway?sslmode=disable

# Asterisk AMI Configuration
ASTERISK_AMI_HOST=localhost
ASTERISK_AMI_PORT=5038
ASTERISK_AMI_USER=admin
ASTERISK_AMI_PASS=adminpass

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=text

# Production Notes:
# - Change GIN_MODE to "release" for production
# - Use strong, randomly generated passwords
# - Consider using environment-specific configuration files
# - Use secrets management for sensitive credentials in production
